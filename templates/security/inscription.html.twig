
{% extends 'base.html.twig' %}
 {% block body %}
 <div class="container mt-3">
 <h2>Sign Up</h2>
 {{ form_start(form) }}


<div class="form-group">
{{ form_row(form.nomClient,{'attr':{'placeholder':'Client name ...','class':'form-control','required':'required'} }) }}
</div>
<div class="form-group">
{{ form_row(form.cinClient,{'attr':{'placeholder':'Client CIN ...','class':'form-control','required':'required'} }) }}
</div>
<div class="form-group">
{{ form_row(form.email,{'attr':{'placeholder':'Client Email ...','class':'form-control','required':'required'} }) }}
</div>
<div class="form-group">
{{ form_row(form.password,{'label':'mot de passe','attr':{'placeholder':'Password ...','class':'form-control','required':'required'} }) }}
</div>
<div class="form-group">
{{ form_row(form.adresse,{'label':'adresse','attr':{'placeholder':'Adress ...','class':'form-control','required':'required'} }) }}
</div>
<div class="form-group">
<button type="submit" class="btn btn-primary">Sign Up</button>
</div>
{{ form_end(form) }}
</div>
{% endblock %}